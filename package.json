{
  "name": "cypressframework",
  "version": "1.0.0",
  "description": "Cypress Framework build for WebUI and API Testing",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "uitest": "cross-env REPORT_NAME=UIReport cypress run --spec 'cypress/e2e/uiTest.cy.js'",
    "apitest": "cross-env REPORT_NAME=APIReport cypress run --spec 'cypress/e2e/apiTest.cy.js'",
    "cypress-dashboard": "npx cypress run --record --key b005b4f8-df95-434c-b706-81a81c42f990",
    "dbtest": "cypress run --spec 'cypress/e2e/databaseTest.cy.js'",
    "allure-report": "cypress run --spec cypress/e2e/TestFacility360.cy.js --env allure=true",
    "browser:chrome": "npx cypress run --browser=chrome --headed --spec cypress/e2e/Facility360/Documents/SmartDMS/TestSmartDMS.cy.js --reporter mocha-allure-reporter",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open  allure-report",
    "allure:clear": "if exist allure-results rmdir /q /s allure-results && if exist allure-report rmdir /q /s allure-report && if exist cypress\\screenshots rmdir /q /s cypress\\screenshots && if exist cypress\\videos rmdir /q /s cypress\\videos",
    "testcases:tag": "npx cypress run --spec cypress/e2e/MonitorSuppliers/TestMonitorSupplier.cy.js --headed --env grepTags=@smokeTag --reporter mocha-allure-reporter"
  },
  "author": "Komal",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "googleapis": "^144.0.0",
    "jsonpath-plus": "^10.0.1",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "moment": "^2.30.1",
    "mysql": "^2.18.1",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@cypress/grep": "^4.1.0",
    "@shelex/cypress-allure-plugin": "^2.40.1",
    "@types/lodash": "^4.14.202",
    "allure-commandline": "^2.27.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.15.0",
    "cypress-email-results": "^1.0.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-grep": "^3.0.4",
    "cypress-map": "^1.40.0",
    "cypress-mochawesome-reporter": "^3.6.0",
    "cypress-xpath": "^2.0.1",
    "jest": "^29.7.0",
    "marge": "^1.0.1",
    "mocha-allure-reporter": "^1.4.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mysql2": "^3.6.1",
    "node-xlsx": "^0.4.0",
    "nodemailer": "^6.9.16"
  }
}
